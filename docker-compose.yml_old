version: '3.3'
volumes:
  'sql03-virtdbnx-asp':
    driver: nimble
    driver_opts:
      importVol: sql03-virtdbnx-asp
      forceImport: "true"
services:
  db:
    image: docker.io/diannemg278/demopod:virtdbnx-asp-db
    build:
      context: .
      dockerfile: virtdbnx_db/Dockerfile
    environment:
      SA_PASSWORD: "Nim123Boli!"
      ACCEPT_EULA: "Y"
    ports:
      - '1410:1433'
    volumes:
      - sql03-virtdbnx-asp:/var/opt/mssql/sqldata/udb
    
